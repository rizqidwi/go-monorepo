{
    "name": "go-monorepo",
    "version": "1.0.0",
    "workspaces": [
        "frontend"
    ],
    "scripts": {
        "dev": "concurrently \"go run cmd/main.go\" \"npm run dev --prefix frontend\"",
        "build": "npm run build --prefix frontend",
        "install:all": "npm install && npm install --prefix frontend"
    },
    "devDependencies": {
        "concurrently": "^9.1.2"
    }
}